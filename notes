az appservice plan create --name shortenurl --resource-group Nintex --sku S1 --is-linux

az webapp create --resource-group Nintex --plan shortenurl --name shorturl --multicontainer-config-type compose --multicontainer-config-file docker-compose.yml

az aks enable-addons --resource-group Nintex --name shortenurl --addons http_application_routing

az aks show --resource-group Nintex --name shorturl --query addonProfiles.httpApplicationRouting.config.HTTPApplicationRoutingZoneName -o table


